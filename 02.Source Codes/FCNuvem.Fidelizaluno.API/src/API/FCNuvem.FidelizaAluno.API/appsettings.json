{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantDomain": "{tenant_domain}",
    "TenantId": "{tenant_id}",
    "ClientId": "{client_id}"
  },
  "Infrastructure": {
    "Email": {
      "User": "{user}",
      "Password": "{password}",
      "Port": "587",
      "Server": "{smtp_server}",
      "AddresFrom": "{address_from}",
      "NameFrom": "{name_from}",
      "Ssl": true,
      "DebugEmail": "{debug_email}"
    },
    "ConnectionString": "{connectionString}"
  },
  "API": {
    "Embedded": {
      "AuthorityUrl": "https://login.microsoftonline.com/common/oauth2/token",
      "ResourceUrl": "https://analysis.windows.net/powerbi/api",
      "ApplicationId": "{appliction_id}",
      "ApiUrl": "https://api.powerbi.com/",
      "WorkspaceId": "{workspace_id}",
      "ReportId": "{report_id}",
      "AuthenticationType": "MasterUser",
      "ApplicationSecret": "{application_secret}",
      "Tenant": "{tenant}",
      "Username": "{username}",
      "Password": "{password}"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
